{
	"name": "n8n-nodes-inboxapp",
	"version": "0.1.0",
	"description": "n8n community node for InboxApp - Social Selling CRM for X",
	"license": "MIT",
	"homepage": "https://inboxapp.com",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "InboxApp",
		"email": "support@inboxapp.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/inboxhq/n8n-nodes-inboxapp.git"
	},
	"scripts": {
		"generate": "node -e \"const{N8NPropertiesBuilder}=require('@devlikeapro/n8n-openapi-node');const d=require('./nodes/InboxApp/openapi.json');const p=new N8NPropertiesBuilder(d,{});require('fs').writeFileSync('./nodes/InboxApp/properties.json',JSON.stringify(p.build(),null,'\\t'))\" && echo 'Properties generated'",
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/InboxAppApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/InboxApp/InboxApp.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
